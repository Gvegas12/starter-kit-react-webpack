{
  "name": "yas-lk",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Khusan Abdukarimov (https://t.me/gvegasss)",
  "license": "MIT",
  "scripts": {
    "start": "webpack serve --env port=3000",
    "build:prod": "webpack --env mode=production",
    "build:stage": "webpack --env mode=stage",
    "build:dev": "webpack --env mode=development --env analyzer=true",
    "preview": "vite preview",
    "lint:ts": "eslint \"**/*.{ts,tsx}\" --report-unused-disable-directives --max-warnings 0",
    "lint:ts:fix": "yarn lint:ts --fix",
    "test:unit": "jest --verbose --config ./config/jest/jest.config.ts",
    "test:ui": "npx loki test",
    "test:ui:ok": "npx loki approve",
    "test:ui:ci": "npx loki --requireReference --reactUri file:./storybook-static",
    "test:ui:report": "npm run test:ui:report:json && npm run test:ui:report:html",
    "test:ui:report:json": "node lib/scripts/reg-cli.generate-visual-json-report.cjs",
    "test:ui:report:html": "reg-cli --from .loki/reg-cli.report.json --report .loki/reg-cli.report.html",
    "prepare": "husky install",
    "create:page": "gvegas-cfc create -t ./lib/templates/AsyncTemplate -o ./src/pages",
    "create:ui": "gvegas-cfc create -t ./lib/templates/UITemplate -o ./src/shared/UI/",
    "create:widget": "gvegas-cfc create -t ./lib/templates/UITemplate -o ./src/widgets/",
    "create:feature": "gvegas-cfc create -t ./lib/templates/UITemplate -o ./src/features/",
    "sb": "storybook dev -p 6006 -c ./config/storybook",
    "build-sb": "storybook build"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.0",
    "@storybook/addon-actions": "^7.4.6",
    "@storybook/addon-essentials": "^7.4.6",
    "@storybook/addon-interactions": "^7.4.6",
    "@storybook/addon-links": "^7.4.6",
    "@storybook/builder-webpack5": "^7.4.6",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^7.4.6",
    "@storybook/testing-library": "^0.2.2",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.3",
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.11",
    "@types/webpack": "^5.28.3",
    "@types/webpack-bundle-analyzer": "^4.6.1",
    "@types/webpack-dev-server": "^4.7.2",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "eslint": "^8.51.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.4.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.14",
    "gvegas-cfc": "^3.0.9",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-dom": "^4.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "loki": "^0.32.0",
    "mini-css-extract-plugin": "^2.7.6",
    "react-docgen-typescript-plugin": "^1.0.5",
    "react-testing-library": "^8.0.1",
    "reg-cli": "^0.17.7",
    "sass": "^1.69.0",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.1",
    "axios": "^1.5.1",
    "clsx": "^2.0.0",
    "dotenv": "^16.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "swr": "^2.2.4",
    "zod": "^3.22.4"
  }
